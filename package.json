{
  "name": "@fastpanel/core",
  "version": "0.0.1",
  "description": "The main core of the \"fastPanel\" system.",
  "author": "Desionlab <fenixphp@gmail.com>",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "scripts": {
    "compile": "./node_modules/.bin/tsc",
    "develop": "./node_modules/.bin/tsc --watch",
    "docs": "./node_modules/.bin/typedoc --theme minimal --target 'esnext' --excludeNotExported --excludePrivate --ignoreCompilerErrors --out ./docs/ ./src/",
    "test": "./node_modules/.bin/mocha --require ts-node/register test/**/*.spec.ts",
    "build": "npm run test && npm run compile && npm run docs",
    "prepare": "npm run test && npm run compile && npm run docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastpanel/core.git"
  },
  "bugs": {
    "url": "https://github.com/fastpanel/core/issues"
  },
  "homepage": "https://github.com/fastpanel/core#readme",
  "keywords": [
    "nodejs",
    "di",
    "cli",
    "worker",
    "typescript"
  ],
  "dependencies": {
    "glob": "^7.1.3",
    "ioredis": "^4.3.0",
    "lodash": "^4.17.11",
    "progress": "^2.0.3",
    "rotating-file-stream": "^1.3.10",
    "shelljs": "^0.8.3",
    "validator": "^10.10.0",
    "vorpal": "^1.12.0",
    "winston": "^3.1.0",
    "winston-daily-rotate-file": "^3.5.1"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/ioredis": "^4.0.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/progress": "^2.0.3",
    "@types/triple-beam": "^1.3.0",
    "@types/validator": "^10.9.0",
    "@types/vorpal": "^1.11.0",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "typedoc": "^0.14.0",
    "typescript": "^3.2.2"
  },
  "license": "MIT"
}
